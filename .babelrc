{
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-runtime"
    ],
    "presets": [
        "stage-0",
        "es2015",
        "react"
    ],
    "env": {
        "production": {
            "plugins": ["transform-react-remove-prop-types"]
        },
        "development": {
            "plugins": ["react-hot-loader/babel"]
        }
    }
}